{
  "name": "elliot-for-water",
  "version": "4.0.0",
  "description": "Search engine with the aim to collect water for less develop countries",
  "repository": "https://bitbucket.org/elliotforwater/elliotforwater_v4/src/master/",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED='0' env-cmd -f .env.development next dev",
    "build:dev": " env-cmd -f .env.development next build",
    "start": "env-cmd -f .env.production next start",
    "build:prod": "env-cmd -f .env.production next build",
    "start:stage": "env-cmd -f .env.staging next build && next start",
    "build:stage": "env-cmd -f .env.staging next build",
    "storybook": "start-storybook -s ./public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:style": "stylelint \"{components, styles}/**/*.css\""
  },
  "keywords": [
    "search",
    "engine",
    "water",
    "planet",
    "environment"
  ],
  "author": "ElliotForWater",
  "license": "ISC",
  "dependencies": {
    "env-cmd": "^10.1.0",
    "next": "^10.0.1",
    "next-translate": "^1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.11.6",
    "@babel/plugin-transform-modules-commonjs": "~7.10.4",
    "@babel/plugin-transform-runtime": "~7.11.5",
    "@babel/preset-flow": "~7.10.4",
    "@storybook/addon-actions": "~6.1.11",
    "@storybook/addon-docs": "~6.1.1",
    "@storybook/react": "~6.1.1",
    "@testing-library/react": "~11.2.2",
    "@types/chrome": "~0.0.127",
    "@types/classnames": "~2.2.10",
    "@types/enzyme": "~3.10.6",
    "@types/jest": "~26.0.14",
    "@types/node": "~14.10.3",
    "@types/react": "~16.9.49",
    "@types/react-dom": "~16.9.8",
    "@typescript-eslint/eslint-plugin": "~4.0.1",
    "@typescript-eslint/parser": "~4.0.1",
    "@wojtekmaj/enzyme-adapter-react-17": "~0.3.1",
    "autoprefixer": "~9.8.6",
    "babel-eslint": "~10.1.0",
    "babel-jest": "~26.3.0",
    "babel-loader": "~8.1.0",
    "classnames": "~2.2.6",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.4",
    "eslint": "~7.9.0",
    "eslint-config-prettier": "~7.0.0",
    "eslint-config-standard": "~14.1.1",
    "eslint-config-standard-jsx": "~8.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jest": "~24.1.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.2.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "~7.21.3",
    "eslint-plugin-react-hooks": "~4.1.2",
    "eslint-plugin-standard": "~4.0.1",
    "fetch-jsonp": "~1.1.3",
    "husky": "~4.3.0",
    "jest": "~26.4.2",
    "js-cookie": "~2.2.1",
    "lint-staged": "~10.3.0",
    "next-ga": "~2.3.4",
    "postcss": "~8.1.1",
    "postcss-extend-rule": "~3.0.0",
    "postcss-flexbugs-fixes": "~4.2.1",
    "postcss-loader": "~4.0.2",
    "postcss-preset-env": "~6.7.0",
    "prettier": "~2.1.1",
    "prettier-stylelint": "~0.4.2",
    "react-device-detect": "~1.14.0",
    "react-hook-form": "~6.14.0",
    "react-icons": "~4.1.0",
    "react-odometerjs": "~2.1.1",
    "storybook": "~6.1.1",
    "storybook-addon-next-router": "~2.0.3",
    "stylelint": "~13.7.2",
    "stylelint-config-standard": "~20.0.0",
    "ts-jest": "~26.4.4",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix --quiet"
    ],
    "*.css": [
      "stylelint"
    ]
  }
}